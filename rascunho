function substituirCondicao(expressao) {
    expressao = expressao.replace(/(\w+)\s*>\s*(\w+)/g, function (match, variavel1, variavel2) {
      return '(!' + variavel1 + ' || ' + variavel2 + ')';
    });
    expressao = expressao.replace(/(\w+)\s*<>\s*(\w+)/g, function (match, variavel1, variavel2) {
      return '(' + variavel1 + ' === ' + variavel2 + ')' + ' && ' + '(' + variavel1 + ' === ' + variavel2 + ')';
    });

    return expressao;
  }